<%= link_to project_path(current_project), class: ["Link"].compact.join(" "), title: t("home") do %>
  <% if current_project&.logo&.file.present? %>
    <%= image_tag(
      current_project.logo.file.variant(resize: [200, 200]),
      class: "SiteHeader-logo",
      alt: "Collection logo")
    %>
  <% end %>
<% end %>
