<%= render partial: "latex/title",
           locals: {
               title: latex_escape(I18n.backend.translate(header_locale,'legend.title')),
               space_above: 0,
               space_below: 0.01,
               font_size: 13
           } %>
<% case interview.project.identifier.to_sym %>
<% when :mog %>
\par{
  \begin{description}[leftmargin=4em,style=nextline]
    \item[\normalfont{( )}] <%= latex_escape(I18n.backend.translate(header_locale,'legend.mog.missed')) %>
    \vspace*{0.2cm}
    \item[\normalfont{[.]}] <%= latex_escape(I18n.backend.translate(header_locale,'legend.mog.comment')) %>
    \vspace*{0.2cm}
    \item[\normalfont{(-)}] <%= latex_escape(I18n.backend.translate(header_locale,'legend.mog.pause_1')) %>
    \vspace*{0.2cm}
    \item[\normalfont{(--)}] <%= latex_escape(I18n.backend.translate(header_locale,'legend.mog.pause_2')) %>
    \vspace*{0.2cm}
    \item[\normalfont{(---)}] <%= latex_escape(I18n.backend.translate(header_locale,'legend.mog.pause_3')) %>
    \vspace*{0.2cm}
    \item[\normalfont{(4)}] Text.<%= latex_escape(I18n.backend.translate(header_locale,'legend.mog.pause_4')) %>
    \vspace*{0.2cm}
    \item[\normalfont{\_}] <%= latex_escape(I18n.backend.translate(header_locale,'legend.mog.assert')) %>
    \vspace*{0.2cm}
    \item[\normalfont{'}] <%= latex_escape(I18n.backend.translate(header_locale,'legend.mog.omission')) %>
  \end{description}
}
<% when :za %>
\par{
  \begin{description}[leftmargin=4em,style=nextline]
    \item[\normalfont{[...]}] <%= latex_escape(I18n.backend.translate(header_locale,'legend.zwar.address')) %>
    \vspace*{0.2cm}
    \item[\normalfont{...}] <%= latex_escape(I18n.backend.translate(header_locale,'legend.zwar.broken_word')) %>
    \vspace*{0.2cm}
    \item[\normalfont{[---]}] <%= latex_escape(I18n.backend.translate(header_locale,'legend.zwar.silence')) %>
    \vspace*{0.2cm}
    \item[\normalfont{(???)}] <%= latex_escape(I18n.backend.translate(header_locale,'legend.zwar.unclear')) %>
    \vspace*{0.2cm}
    \item[\normalfont{(<%= I18n.backend.translate(header_locale, :word) %> ?)}] <%= latex_escape(I18n.backend.translate(header_locale,'legend.zwar.unsure')) %>
    \vspace*{0.2cm}
    \item[\normalfont{[x]}] <%= latex_escape(I18n.backend.translate(header_locale,'legend.zwar.break')) %>
    \vspace*{0.2cm}
    \item[\textit{<%= I18n.backend.translate(header_locale, :italic) %>}] <%= latex_escape(I18n.backend.translate(header_locale,'legend.zwar.italic')) %>
    \vspace*{0.2cm}
    \item[\normalfont{"xyz"}] <%= latex_escape(I18n.backend.translate(header_locale,'legend.zwar.quote')) %>
    \vspace*{0.2cm}
    \item[\normalfont{{<%= I18n.backend.translate(header_locale, :text) %>} <%= I18n.backend.translate(header_locale, :or) %> {(<%= I18n.backend.translate(header_locale, :text) %>)}}] <%= latex_escape(I18n.backend.translate(header_locale,'legend.zwar.annotation')) %>
    \vspace*{0.2cm}
    \item[\normalfont{<***>}] <%= latex_escape(I18n.backend.translate(header_locale,'legend.zwar.tape_end')) %>
    \vspace*{0.2cm}
  \end{description}
}
<% else %>
\par{
  \begin{description}[leftmargin=4em,style=nextline]
    \item[\normalfont{<i>}] <%= latex_escape(I18n.backend.translate(header_locale,'legend.default.interruption')) %>
    \vspace*{0.2cm}
    \item[\normalfont{<p2>, <p3> etc.}] <%= latex_escape(I18n.backend.translate(header_locale,'legend.default.pause')) %>
    \vspace*{0.2cm}
    \item[\normalfont{<?2>, <?3> etc.}] <%= latex_escape(I18n.backend.translate(header_locale,'legend.default.unclear')) %>
    \vspace*{0.2cm}
    \item[\normalfont{\_}] <%= latex_escape(I18n.backend.translate(header_locale,'legend.default.broken_word')) %>
    \vspace*{0.2cm}
    \item[\normalfont{<=>}] <%= latex_escape(I18n.backend.translate(header_locale,'legend.default.fast_word')) %>
    \vspace*{0.2cm}
    \item[\normalfont{<l(es) xyz>; <l(es-de)>; <l(de-es)>}] <%= latex_escape(I18n.backend.translate(header_locale,'legend.default.lexical_phenomenon')) %>
    \vspace*{0.2cm}
    \item[\normalfont{<s(<%= I18n.backend.translate(header_locale, 'legend.default.vocal_shift') %>) xyz>}] <%= latex_escape(I18n.backend.translate(header_locale,'legend.default.changed_voice')) %>
    \vspace*{0.2cm}
    \item[\normalfont{<sim>}] <%= latex_escape(I18n.backend.translate(header_locale,'legend.default.simultaneous')) %>
    \vspace*{0.2cm}
    \item[\normalfont{<nl(<%= I18n.backend.translate(header_locale, 'legend.default.sound') %>) xyz>}] <%= latex_escape(I18n.backend.translate(header_locale,'legend.default.non_linguistic')) %>
    \vspace*{0.2cm}
    \item[\normalfont{<v(<%= I18n.backend.translate(header_locale, 'legend.default.fonetic_expression') %>)>}] <%= latex_escape(I18n.backend.translate(header_locale,'legend.default.non_verbal')) %>
    \vspace*{0.2cm}
    \item[\normalfont{<g(<%= I18n.backend.translate(header_locale, 'legend.default.gesture_type') %>) xyz>}] <%= latex_escape(I18n.backend.translate(header_locale,'legend.default.gesture')) %>
    \vspace*{0.2cm}
    \item[\normalfont{<m(<%= I18n.backend.translate(header_locale, 'legend.default.expression_type') %>) xyz>}] <%= latex_escape(I18n.backend.translate(header_locale,'legend.default.facial_expression')) %>
    \vspace*{0.2cm}
    \item[\normalfont{<n>}] <%= latex_escape(I18n.backend.translate(header_locale,'legend.default.notes')) %>
    \vspace*{0.2cm}
  \end{description}
}
<% end %>

